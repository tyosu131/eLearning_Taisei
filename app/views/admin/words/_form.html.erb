<%= form_with(model: [:admin, @category, @word], local: true) do |f| %>
    <p class="text-left">Words</p>
    <%= f.text_field :word, class:"form-control" %>
    <%# <p class="text-left">Choice</p> %>
    <%#= f.text_field :choice, class:"form-control" %>

    <%= f.submit :submit, class: 'btn btn-primary' %>
    <%= link_to "back", admin_category_path(@category), class: 'btn btn-danger' %>
<% end %>